{
  "parser": "babel-eslint",
  "root": true,
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "import",
  ],
  "env": {
    "browser": true,
    "node": true,
  },
  "globals": {
    "__DEV__": false
  },
  "rules": {
    "function-paren-newline": 0,
    "import/prefer-default-export": 0,
    "jsx-a11y/label-has-for": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "object-curly-newline": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unused-state": 0,
  },
}
